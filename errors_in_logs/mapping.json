{
  "mappings": {
    "logs": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "strict_date_optional_time||epoch_millis"
        },
        "message": {
          "type": "string"
        },
        "loglevel":{
          "type": "string",
          "index":"not_analyzed"
        }
      }
    }
  }
}